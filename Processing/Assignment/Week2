// Write an application that loops from 1 to 100. void function cannot return "value"
for (int a = 1; a <= 100; a++) {
  if ((a % 3 == 0) && (a % 15 != 0)) {
    println("Media");
  }
  if ((a % 5 == 0) && (a % 15 !=0)) {
    println("Technology");
  }
  if (a % 15 == 0) {
    println("Media Technology");
  }
  else if((a % 3 != 0) && (a % 5 != 0)) {
    println(a) ; 
  }
}
